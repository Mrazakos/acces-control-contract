{
  "name": "access-control-hardhat",
  "version": "1.0.0",
  "description": "Solidity implementation of a smart contract for an access control system.",
  "main": "index.js",
  "scripts": {
    "test": "hardhat test",
    "test:unit": "npx hardhat test .\\test\\unit\\AccessControl.crypto.test.ts",
    "test:load": "npx hardhat test .\\test\\load\\AccessControl.load.stress.test.ts",
    "test:benchmark": "npx hardhat test .\\test\\benchmark\\AccessControl.benchmark.gas.test.ts",
    "test:integration": "npx hardhat test .\\test\\integration\\*.test.ts",
    "test:performance": "npx hardhat test .\\test\\performance\\*.test.ts",
    "test:all": "hardhat test",
    "test:gas": "set REPORT_GAS=true&& hardhat test",
    "report": "ts-node scripts/generate-report.ts",
    "test:full": "npm run test:all && npm run report",
    "deploy": "hardhat run scripts/deploy.ts --network localhost",
    "compile": "hardhat compile"
  },
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.6",
    "@typechain/ethers-v5": "^10.2.1",
    "@typechain/hardhat": "^6.1.6",
    "@types/chai": "^5.2.2",
    "@types/mocha": "^10.0.10",
    "@types/node": "^14.0.0",
    "chai": "^6.0.1",
    "ethereum-waffle": "^4.0.10",
    "ethers": "^5.0.0",
    "hardhat": "^2.0.0",
    "hardhat-gas-reporter": "^2.3.0",
    "ts-node": "^10.0.0",
    "typechain": "^8.3.2",
    "typescript": "^4.0.0"
  },
  "dependencies": {
    "@mrazakos/vc-ecdsa-crypto": "^3.1.0",
    "@openzeppelin/contracts": "^4.9.6",
    "dotenv": "^17.2.2"
  }
}
